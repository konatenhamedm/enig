{% block page_content %}    {% form_theme form 'widget/fields-block.html.twig' %}    <div class="modal-header">        <button type="button" class="close" data-dismiss="modal" aria-hidden="true"></button>        <h4 class="modal-title">Suppression</h4>    </div>        <div class="modal-body">             {{ include('admin/ajax/response.html.twig') }}            <p>Voulez-vous vraiment supprimer cet enregistrement</span>                 <input type="hidden" name="cacher" value="{{ courierArrive }}">        </div>        <div class="modal-footer">         {#   {{ include('admin/ajax/loader.html.twig') }}#}            <button type="button" class="btn dark btn-outline" data-dismiss="modal">Fermer</button>            <button type="submit" class="btn btn-danger supprimer">Supprimer</button>        </div>{% endblock %}{% block javascripts %}    <script src="{{ asset('assets/admin/js/jquery3.1.js') }}"></script>    <script>        const value = $('.cacher').val();        console.log(value)        $.ajax({            url: '/admin/existe',            type: 'get',            dataType: 'json',            success: function (response, status) {                $('#principal .mot').html('Décochez cette option pour une numérotation personnalisée');                $('#courier_arrive_numero').val(response.nom)                $('#courier_arrive_numero').attr('readonly', true)                //console.log(response.nom);            },            error: function (error) {                console.log(error);            }        });            </script>{% endblock %}